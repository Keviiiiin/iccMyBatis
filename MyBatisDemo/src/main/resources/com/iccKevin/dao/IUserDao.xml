<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!--命名空间写dao接口的全限定类名-->
<!--可能有多个数据库表需要查询，所以需要将命名空间和id组合在一起才能定位到SQL语句-->
<mapper namespace="com.iccKevin.dao.IUserDao">
    <!--配置查询所有-->
    <select id="findAll" resultType="com.iccKevin.domain.User">
        select * from user
    </select>
</mapper>